# 1'den 5'e kadar olan sayıları paralel olarak işleme
$sayilar = 1..5

# Paralel işlem başlatma
$sayilar | ForEach-Object -Parallel {
    Start-Sleep -Seconds 10 # Her işlem 10 saniye bekler
    $kare = $_ * $_
    Write-Output "Sayı: $_, Karesi: $kare"
}

# Paralel işlemler tamamlandıktan sonra yürütülecek kod
Write-Output "Paralel işlemler tamamlandı. Şimdi sıradaki kod çalışıyor."

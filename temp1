$cluster="GBTKSCL"
get-cluster $cluster | Get-VIPermission | Where-Object{
$row = "" | Select-Object Cluster, Principal, Role, Inherited
$row.cluster = $cluster
$row.Principal = $_.Principal
$row.Role = $_.Role
$row.Inherited = ($_.EntityId -ne $cls.Id)
$report += $row
}

$report | Export-Csv E:\Murat\test\prod-perm_20012025.csv

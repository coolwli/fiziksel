CREATE TABLE DailySummary (
    SummaryDate DATE NOT NULL,
    Column1 NVARCHAR(MAX), -- Eğer text ise "a'10~b'3" gibi özel karakterlerle saklanacak
    Column2 INT,           -- Eğer integer ise toplam değeri saklayacak
    Column3 NVARCHAR(MAX), -- Aynı şekilde text veya integer olabilir
    -- Diğer sütunlar
    PRIMARY KEY (SummaryDate)
);

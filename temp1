# Oracle bağlantı bilgilerini tanımlıyoruz
$oracleConnectionString = "Driver={Oracle in OraClient19Home1};Dbq=YOUR_ORACLE_DB;Uid=YOUR_USERNAME;Pwd=YOUR_PASSWORD;"

# Oracle bağlantısını oluşturuyoruz
$connection = New-Object System.Data.Odbc.OdbcConnection
$connection.ConnectionString = $oracleConnectionString

# Bağlantıyı açıyoruz
$connection.Open()

# Sorguyu tanımlıyoruz
$query = "SELECT * FROM SCHEMA_NAME.TABLE_NAME WHERE ROWNUM <= 10"  # Örneğin ilk 10 satırı çekelim

# Komutu hazırlıyoruz
$command = $connection.CreateCommand()
$command.CommandText = $query

# Sorguyu çalıştırıp sonuçları çekiyoruz
$adapter = New-Object System.Data.Odbc.OdbcDataAdapter($command)
$dataset = New-Object System.Data.DataSet
$adapter.Fill($dataset)

# Sonuçları gösteriyoruz
$dataset.Tables[0] | ForEach-Object { $_ }

# Bağlantıyı kapatıyoruz
$connection.Close()



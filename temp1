using System;
using System.Data.Odbc;
using System.Web.UI;

public partial class YourPage : System.Web.UI.Page
{
    protected void Page_Load(object sender, EventArgs e)
    {
        string connectionString = "Driver={Oracle ODBC Driver};Dbq=yourDB;Uid=yourUserId;Pwd=yourPassword;";

        try
        {
            using (OdbcConnection conn = new OdbcConnection(connectionString))
            {
                conn.Open();

                string query = "SELECT * FROM your_table_name";
                OdbcCommand cmd = new OdbcCommand(query, conn);
                OdbcDataReader reader = cmd.ExecuteReader();

                string resultHtml = "";

                while (reader.Read())
                {
                    // Veritabanı verisini HTML formatında hazırlıyoruz
                    resultHtml += "<tr><td>" + reader["column_name"].ToString() + "</td></tr>";
                }

                // HTML sonucu sayfadaki tbody'ye ekliyoruz
                results.InnerHtml = resultHtml;
            }
        }
        catch (Exception ex)
        {
            // Hata mesajını HTML'e ekleyelim
            results.InnerHtml = "<tr><td colspan='1'>Hata: " + ex.Message + "</td></tr>";
        }
    }
}




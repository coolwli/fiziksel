$baseUri = "https://<vrops-server>/suite-api/api"
$credentials = [Convert]::ToBase64String([Text.Encoding]::ASCII.GetBytes("username:password"))

$headers = @{
    Authorization = "Basic $credentials"
    Accept        = "application/json"
}

$response = Invoke-RestMethod -Uri "$baseUri/resources" -Method Get -Headers $headers

# Çekilen verileri görüntüleme
$response

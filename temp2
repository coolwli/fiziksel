        private Dictionary<string, Dictionary<string, int[]>[]>[] GetAllDatasets(string query)
        {
            Dictionary<string, Dictionary<string, int[]>[]>[] datasets;
            using (var con = new SqlConnection(connectionString))
            {
                con.Open();
                using (var cmd = new SqlCommand(query, con))
                {
                    using (var reader = cmd.ExecuteReader())
                    {
                        while (reader.Read())
                        {
                            //burada her bir sütun için Dictionary<string, Dictionary<string, int[]>[]> oluşturalacak ve bunların birleşimi döndğrğlecek
                            //stringe o kolonun adı yazılacak, Dictionary<string, int[]>[] kısmındaysa dinamik a|10~ba|10~ca|10~ gibi gün gün tutulan isimleri aynı
                            //değışkende toplayıp mesela "a",[10,4,70] gibi tum satırlara bakıp oluşturulacak
                        }
                    }
                }
            }

            return datasets;
        }

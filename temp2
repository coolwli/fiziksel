private string ParseVmIdentifier(string xmlData)
{
    XDocument xdoc = XDocument.Parse(xmlData);
    XNamespace opsNamespace = "http://webservice.vmware.com/vRealizeOpsMgr/1.0/";

    var identifier = xdoc.XPathSelectElement("//ops:resource/ops:identifier", new XmlNamespaceManager(new NameTable()));
    return identifier?.Value;
}
